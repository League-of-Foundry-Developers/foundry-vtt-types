    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsdoc": "^50.6.16",
    "eslint-plugin-tsdoc": "^0.4.0",
    "globals": "^16.0.0",
    "husky": "^9.1.4",
    "is-ci": "^4.1.0",
    "lint-staged": "^16.0.0",
    "npm-run-all2": "^8.0.1",
    "pathe": "^2.0.2",
    "prettier": "^3.0.3",
    "type-fest": "^4.34.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.31.0",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "typescript": "^5.4"
  },
  "overrides": {
    "@types/css-font-loading-module": "^0.0.14",
    "@pixi/utils": {
      "@types/earcut": "3.0.0"
    }
  },
  "files": [
    "index.d.mts",
    "index-lenient.d.mts",
    "src",
    "tsconfig.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "npm@10.8.1"
}
